\begin_layout Standard

\size small
\begin_inset ERT
status open

\begin_layout Plain Layout


\backslash
epigraph{
\backslash
textit{I find [being]...
 ...out in the open so beneficial.
 Even if it is but for a minute or two, it is unbelievable how that short
 time helps to...
 ...bring feelings into equilibrium...}}{
\backslash
textsc{Roald Amundsen on expedition to the South Pole}}
\end_layout

